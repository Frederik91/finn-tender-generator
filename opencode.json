{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "finn-no": {
      "type": "local",
      "command": ["npx", "-y", "github:viktorfa/finn_mcp"],
      "enabled": true,
      "environment": {
        "MY_ENV_VAR": "value"
      }
    },
    "image-convertor-mcp": {
      "type": "local",
      "command": ["uvx", "image-convertor-mcp"],
      "enabled": true
    }
  }
}